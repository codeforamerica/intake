{
  "name": "intake",
  "env": {
    "ALLOWED_HOSTS": {
      "value": "*"
    },
    "AWS_ID": {
      "required": false
    },
    "AWS_SECRET": {
      "required": false
    },
    "DATABASE_HOST": {
      "required": false
    },
    "DATABASE_PASSWORD": {
      "required": false
    },
    "DATABASE_URL": {
      "required": false
    },
    "DEFAULT_HOST": {
      "required": true
    },
    "DIVERT_REMOTE_CONNECTIONS": {
      "value": "True",
      "required": false
    },
    "DJANGO_SETTINGS_MODULE": {
      "required": true
    },
    "FRONT_API_TOKEN": {
      "value": "aaaaaa"
    },
    "FRONT_EMAIL_CHANNEL_ID": {
      "value": "aaaaaa"
    },
    "FRONT_PHONE_CHANNEL_ID": {
      "value": "aaaaaa"
    },
    "MAIL_DEFAULT_SENDER": {
      "required": true
    },
    "MAILGUN_PRIVATE_API_KEY": {
      "required": true
    },
    "MEDIA_BUCKET": {
      "required": false
    },
    "MIXPANEL_KEY": {
      "required": false
    },
    "PARTNERSHIPS_LEAD_INBOX": {
      "required": false
    },
    "PURGED_DATABASE_HOST": {
      "required": false
    },
    "PURGED_DATABASE_PASSWORD": {
      "required": false
    },
    "PURGED_DATABASE_USER": {
      "required": false
    },
    "SECRET_KEY": {
      "generator": "secret",
      "required": false
    },
    "SENDGRID_API_KEY": {
      "required": true
    },
    "SENTRY_URL": {
      "required": true
    },
    "SERVER_EMAIL": {
      "required": true
    },
    "SLACK_WEBHOOK_URL": {
      "value": "aaaaaa"
    },
    "TEST_USER_PASSWORD": {
      "required": false
    },
    "TWILIO_AUTH_TOKEN": {
      "required": true
    },
    "VOICEMAIL_NOTIFICATION_EMAIL": {
      "required": false
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    },
    "worker": {
      "quantity": 1
    }
  },
  "addons": ["cloudamqp"],
  "environments": {
    "review": {
      "addons": [
        "heroku-postgresql:hobby-dev",
        "cloudamqp",
        "bucketeer"
      ]
    }
  },
  "buildpacks": [
    {
      "url": "https://github.com/heroku/heroku-buildpack-jvm-common.git"
    },
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "heroku/python"
    }
  ],
  "scripts": {
    "postdeploy": "python manage.py prepare_review_env"
  }
}
