{%- if county_list %}
<section class="county_notice">
<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-8 col-md-6 col_centered">
      <p class="application_county_list">
      <em>
        {{ _("You are applying for help in ") -}}
        {{- oxford_comma(county_list) }}. <a href="{{ url('intake-apply') }}">{{ _("Switch counties") }}</a>.
      </em>
      </p>
    </div>
  </div>
</div>
</section>
{%- endif %}