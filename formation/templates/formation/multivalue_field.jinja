<div id="{{ field.get_html_class_name() }}" class="field field-multivalue_fieldset {{ field.get_html_class_name() }}
  {%- if field.warnings %} warnings{% endif %}
  {%- if field.errors %} errors{% endif %}">
  {%- include "formation/messages_list.jinja" %}
  <fieldset>
    <legend>{{ field.label }}
      {%- if field.optional %}
      <span class="field-optional">(optional)</span>
      {%- endif %}
    </legend>
    {%- if field.help_text %}
    <div class="field-help_text">
      {{ field.help_text }}
    </div>
    {%- endif %}
    <div class="subfield-row">
    {%- for subfield in field.subfields %}
      {{ subfield.render()|indent(6)|safe }}
    {%- endfor %}
    </div>
  </fieldset>
</div>